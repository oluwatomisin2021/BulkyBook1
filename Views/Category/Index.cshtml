@model IEnumerable<Category>
@{
    Layout = "_Layout";
    ViewData["Title"] = "Index"; 
}

<h1>Index Category</h1>

<table class="table table-striped table-bordered" style="width:100%">
    <thead>
        <tr>
            <th>Category Name</th>
            <th>Display Order</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
           <tr>
               <td style="max-width:50%;">@obj.Name</td>
               <td style="max-width:50%;">@obj.DisplayOrder</td>
           </tr> 
        }
    </tbody>
</table>